    <!-- 使用了类型别名 -->
    <select id="findTokenByTokenString" resultType="Token" parameterType="string">
        select * from token where token=#{xx}
    </select>

    <insert id="save" parameterType="Token" useGeneratedKeys="true"  keyProperty="id">
        insert into token(createTime, ownerName, ownerId, token)
        values(#{id}, #{createTime}, #{ownerName},#{ownerId}, #{token})
    </insert>
